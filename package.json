{
  "name": "scientific-research-assistant",
  "version": "1.0.0",
  "description": "AI-powered scientific research desktop application with multi-agent paper generation",
  "main": "electron/main.js",
  "scripts": {
    "start": "concurrently \"npm run start:react\" \"npm run start:electron\" \"npm run start:backend\"",
    "start:react": "vite",
    "start:electron": "wait-on -t 60000 http://localhost:5173 && electron .",
    "start:backend": "cd backend && /Users/gxl/anaconda3/bin/python main.py",
    "build": "vite build",
    "build:electron": "electron-builder",
    "package": "npm run build && electron-builder --win --mac --linux"
  },
  "keywords": [
    "research",
    "ai",
    "multi-agent",
    "paper-generation",
    "electron"
  ],
  "author": "Scientific Research Team",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "antd": "^5.12.8",
    "axios": "^1.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-quill": "^2.0.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.scientific.research.assistant",
    "productName": "Scientific Research Assistant",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "build/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  }
}